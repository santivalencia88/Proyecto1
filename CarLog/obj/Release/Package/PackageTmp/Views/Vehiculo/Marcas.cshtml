@model IEnumerable<CarLog.Models.marca>

@{
    ViewBag.Title = "Marcas";
}

<h2>Detalle Marcas</h2>


<table class="table">
    <tr>
        <th>
            <p>Marca</p>
        </th>
        <th>
            <p>Detalle</p>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.id_marca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.detalle_marca)
        </td>
    </tr>
}

</table>
